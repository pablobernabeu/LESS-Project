#Session 3, N300

#creation of regex pattern in order for the programme to look for the pattern of participantNumber_ERP in two different properties and two different conditions in order to create two sets of data
#pattern1 (S1_S101) tests gender agreement in the Grammatical condition
#pattern2 (S1_S102) tests gender agreement in the Ungrammatical condition
#pattern3 (S2S101) tests differential object marking in the Grammatical condition
#pattern4 (S2S102) tests differential object marking in the Ungrammatical condition


pattern1_S3_N300 = "^[0-9].*N300_S1_S101.txt"
print(pattern1_S3_N300)
pattern2_S3_N300 = "^[0-9].*N300_S1_S102.txt"
pattern3_S3_N300 = "^[0-9].*N300_S2_S102.txt"
pattern4_S3_N300 = "^[0-9].*N300_S2_S102.txt"

#creation of two tables on the property of gender, one for the Grammatical and one for the Ungrammatical condition
file_listS101_S3_N300 <- list.files(pattern = pattern1_S3_N300)
print(file_listS101_S3_N300)

file_listS102_S3_N300 <- list.files(pattern = pattern2_S3_N300)
print(file_listS102_S3_N300)


#creation of two tables on the property of differential object marking, one for the Grammatical and one for the Ungrammatical condition
file_listS103_S3_N300 <- list.files(pattern = pattern3_S3_N300)
print(file_listS103_S3_N300)

file_listS104_S3_N300 <- list.files(pattern = pattern4_S3_N300)
print(file_listS104_S3_N300)


#reading the tables
data_listS101_S3_N300 <- lapply(file_listS101_S3_N300, function(file) {
  read.table(file, header = TRUE, sep = "")
})

data_listS102_S3_N300 <- lapply(file_listS102_S3_N300, function(file) {
  read.table(file, header = TRUE, sep = "")
})

data_listS103_S3_N300 <- lapply(file_listS103_S3_N300, function(file) {
  read.table(file, header = TRUE, sep = "")
})

data_listS104_S3_N300 <- lapply(file_listS104_S3_N300, function(file) {
  read.table(file, header = TRUE, sep = "")
})

#Checking dimensions of the data frames

str(data_listS101_S3_N300[[1]])
str(data_listS102_S3_N300[[1]])
str(data_listS103_S3_N300[[1]])
str(data_listS104_S3_N300[[1]])


#checking if rows are missing and removing them

if (all(sapply(data_listS101_S3_N300, nrow) > 0)) {
  S3_S101_N300 <- Reduce(function(...) merge(..., all = TRUE), data_listS101_S3_N300)
  View(S3_S101_N300)
} else {
  print("Some data frames in data_listS101_S3_N300 have no rows.")
}


if (all(sapply(data_listS102_S3_N300, nrow) > 0)) {
  S3_S102_N300 <- Reduce(function(...) merge(..., all = TRUE), data_listS102_S3_N300)
  View(S3_S102_N300)
} else {
  print("Some data frames in data_list2_S3_N300 have no rows.")
}


if (all(sapply(data_listS103_S3_N300, nrow) > 0)) {
  S3_S103_N300 <- Reduce(function(...) merge(..., all = TRUE), data_listS103_S3_N300)
  View(S3_S103_N300)
} else {
  print("Some data frames in data_listS101_S3_N300 have no rows.")
}


if (all(sapply(data_listS104_S3_N300, nrow) > 0)) {
  S3_S104_N300 <- Reduce(function(...) merge(..., all = TRUE), data_listS104_S3_N300)
  View(S3_S104_N300)
} else {
  print("Some data frames in data_listS101_S3_N300 have no rows.")
}

#creating tables for each dataset
#GEN encoding = Gender
#DOM encoding = Differential Object Marking

S3_GEN_Grammatical_N300 <- do.call(rbind, data_listS101_S3_N300)
str(S3_GEN_Grammatical_N300)
head(S3_GEN_Grammatical_N300)

S3_GEN_Ungrammatical_N300 <- do.call(rbind, data_listS102_S3_N300)
head(S3_GEN_Ungrammatical_N300)

S3_DOM_Grammatical_N300 <- do.call(rbind, data_listS103_S3_N300)
head(S3_DOM_Grammatical_N300)

S3_DOM_Ungrammatical_N300 <- do.call(rbind, data_listS104_S3_N300)
head(S3_DOM_Ungrammatical_N300)

#converting the tables into data frames
S3_GEN_Grammatical_N300 <- as.data.frame(S3_GEN_Grammatical_N300)
S3_GEN_Ungrammatical_N300 <- as.data.frame(S3_GEN_Ungrammatical_N300)
S3_DOM_Grammatical_N300 <- as.data.frame(S3_DOM_Grammatical_N300)
S3_DOM_Ungrammatical_N300 <- as.data.frame(S3_DOM_Ungrammatical_N300)

#testing dataframes
View(S3_GEN_Grammatical_N300)
View(S3_GEN_Ungrammatical_N300)
View(S3_DOM_Grammatical_N300)
View(S3_DOM_Grammatical_N300)

#the decimal point is a comma in BrainVision analyzer export files
#converting commas to periods in the dataframe
S3_GEN_Grammatical_N300 <- S3_GEN_Grammatical_N300 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))
S3_GEN_Ungrammatical_N300 <- S3_GEN_Ungrammatical_N300 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))
S3_DOM_Grammatical_N300 <- S3_DOM_Grammatical_N300 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))
S3_DOM_Ungrammatical_N300 <- S3_DOM_Ungrammatical_N300 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))

#Converting columns to numeric if necessary
S3_GEN_Grammatical_N300 <-S3_GEN_Grammatical_N300 %>% mutate(across(everything(), as.numeric))
S3_GEN_Ungrammatical_N300 <- S3_GEN_Ungrammatical_N300 %>% mutate(across(everything(), as.numeric))
S3_DOM_Grammatical_N300 <-S3_DOM_Grammatical_N300 %>% mutate(across(everything(), as.numeric))
S3_DOM_Ungrammatical_N300 <- S3_DOM_Ungrammatical_N300 %>% mutate(across(everything(), as.numeric))

# dividing electrode placements into areas of interest
# averaging all rows and columns so that only one value per region
# Extracting columns and converting to numeric
# Calculating row-wise means excluding NA
# Calculating the overall mean of row means
# TP9, TP10 have been removed from the left and right posterior areas, as they have been used for the re-referencing across sessions
# removed as per the recommendation of Brain Vision products in order to avoid rejecting segments

# For Gender

# For Gender in the Grammatical condition

