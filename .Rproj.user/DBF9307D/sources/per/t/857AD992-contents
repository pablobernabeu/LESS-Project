S3_DOM_Grammatical_N200 <- S3_DOM_Grammatical_N200 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))
S3_DOM_Ungrammatical_N200 <- S3_DOM_Ungrammatical_N200 %>% 
  mutate(across(everything(), ~ gsub(",",".",., fixed = TRUE)))

